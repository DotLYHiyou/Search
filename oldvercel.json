{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chemical.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/chemical.components.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/chemical.cloaking.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
