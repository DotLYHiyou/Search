{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/testcafe-hammerhead/lib/client/**",
          "node_modules/testcafe-hammerhead/lib/client-scripts/**"
        ]
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chemical.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/chemical.components.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/chemical.cloaking.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.[a-zA-Z0-9]+)$",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
